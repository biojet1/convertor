<sub id="length" name="Length" no="D302">

	<unit factor="1.852E+03" name="International Nautical Mile" no="E74F" symbol="n mile">
		A unit of length used in navigation which is equivalent to the distance spanned by one second of arc in latitude.
		Adopted by the First International Extraordinary Hydrographic Conference, Monaco, 1929
	</unit>

	<unit factor="1.853184E+03" name="Nautical Mile (UK)" no="DABE" symbol="n mile (UK)" eval="0.3048*6080">
		6080 ft
		<!-- (6080ft/12in)*(1in/1.0E-02m) -->
		<same equals="6080" no="F3D5"/>
	</unit>

	<unit factor="1.609344E+03" name="Mile" no="9EA2" symbol="mi" eval="0.3048*5280">
		5280 ft
		<same equals="5280" no="F3D5" name="Foot"/>
	</unit>

	<unit factor="5.556E+03" name="League (International nautical)" no="BFF2" eval="1.852E+03*3">
		3 International nautical mile
		<same equals="3" no="E74F" name="International Nautical Mile"/>
	</unit>

	<unit factor="5.559552E+03" name="League (UK nautical)" no="9E6D" eval="1.853184E+03*3">
		3 Nautical mile (UK)
		<same equals="3" no="DABE" name="Nautical Mile (UK)"/>
	</unit>

	<unit factor="4.828032E+03" name="League (US)" no="8FD3" eval="(1200/3937)*5280*3">
		3 mi
	</unit>

	<unit factor="9.144E+02" name="Kiloyard" no="9DB8" symbol="kyd" eval="0.3048*3000">
		3000 ft
		<same equals="3000" no="F3D5" name="Foot"/>
	</unit>

	<unit factor="2.01168E+02" name="Furlong" no="8B77" eval="0.3048*660">
		660 ft = 10 ch
		<same equals="660" no="F3D5" name="Foot"/>
	</unit>

	<unit factor="2.01168E+01" name="Chain" no="C521" symbol="ch" eval="0.3048*66">
		66 ft = 22 yd
		<same equals="66" no="F3D5" name="Foot"/>
		<same equals="22" no="91F4" name="Yard"/>
	</unit>

	<unit factor="5.0292E+00" name="Rod" no="D08F" symbol="rd"  eval="0.3048*16.5">
