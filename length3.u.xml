	</unit>

	<unit factor="2.01168E-01" name="Link" no="903F" eval="0.3048*0.66">
		0.66 ft = 1/100 ch
		<same equals="0.66" no="F3D5"/>
		<same equals="0.01" no="C521"/>
	</unit>

	<unit factor="1.016E-01" name="Hand" no="CD07" eval="0.0254*4">
